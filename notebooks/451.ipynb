    "def ans(N):\n",
    "    T = 0\n",
    "    for n in range(3, N + 1):\n",
    "        ma = 0\n",
    "        for q in range(1, n - 1):\n",
    "            if q * q % n == 1:\n",
    "                ma = max(ma, q)\n",
    "        T += ma\n",
    "    return T"
   ]
