   "source": [
    "def isp(n):\n",
    "    for i in range(2, int(n**0.5+1)):\n",
    "        if n % i == 0:\n",
    "            return False\n",
    "    return True"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "0bc8e7cc-f491-4d4b-84d7-459be675458f",
   "metadata": {},
   "outputs": [],
   "source": [
    "primes = [i for i in range(1000, 5000 + 1) if isp(i)]"
   ]
