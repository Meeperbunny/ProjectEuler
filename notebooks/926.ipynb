    "def R(n):\n",
    "    t = 0\n",
    "    for b in range(2, n + 1):\n",
    "        k = 0\n",
    "        while n % b**(k+1) == 0:\n",
    "            k += 1\n",
    "        t += k\n",
    "    return t"
   ]
